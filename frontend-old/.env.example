# URL del Backend API
REACT_APP_BACKEND_URL=http://localhost:8001

# URL para WebSocket (para actualizaciones en tiempo real)
REACT_APP_WEBSOCKET_URL=ws://localhost:8001

# Configuración de la aplicación
REACT_APP_NAME=Centro Cultural Banreservas
REACT_APP_VERSION=1.0.0

# Features flags (opcional)
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_DEBUG=false

# Límites de la aplicación
REACT_APP_MAX_FILE_SIZE_MB=10
REACT_APP_SESSION_TIMEOUT_MINUTES=30

# URLs públicas (opcional)
REACT_APP_PUBLIC_URL=http://localhost:3000
REACT_APP_SUPPORT_EMAIL=soporte@culturalcenter.com